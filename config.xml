<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.phonegap.example"
        versionCode = "10" 
        version     = "1.0.0" >
  <!--versionCode is optional and Android only-->
  <name>3er Parcial 5toE app piano</name>
   
  <descripcion>
    Utilizar plugins de audio con un piano y las notas musicales 
	</descripcion>
	
	<preference name="phonegap-version" value="3.1.0"/>
	<!--forzar la orientacion a horizontal-->
	<preference name="orientation" value="landscape"/>
	
	<author href="http://build.phonegap.com" email="support@phonegap.com">
	Mariana Pomposo Gonsaga
	</author> 
	
	<gap:plugin name="org.apache.cordova.vibration" version="0.3.0"/>
	<gap:plugin name="org.apache.cordova.dialog" version="0.2.6"/>
	<gap:plugin name="com.rjfun.cordova.plugin.lowlatencyaudio" version="1.0"/>
	
	
</widget>
